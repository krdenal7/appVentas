<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/devoluciones_full_indicadores"
    android:background="#3B3B3B"
    tools:context=".DevolucionesFull.DevolucionesFullIndicadores"
    android:weightSum="1"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="10dp">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_above="@+id/idButtonIndicadores"
            android:layout_alignParentTop="true">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="@string/txtDevFullAverageSales"
                android:id="@+id/idTxtDevFullAverageSales"
                android:textStyle="normal"
                android:textColor="#FFFFFF"
                android:layout_weight="0"
                android:layout_gravity="left|center_vertical" />

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:layout_gravity="center_vertical"
                android:gravity="center_horizontal"
                android:background="#7D7E7d"
                android:paddingRight="5dp"
                android:paddingLeft="5dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="$"
                    android:id="@+id/textView125"
                    android:layout_weight="0"
                    android:layout_gravity="center_vertical"
                    android:shadowColor="#000000"
                    android:textColor="#FFFFFF"
                    android:layout_marginRight="5dp"
                    android:textStyle="bold" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="30,023.00"
                    android:id="@+id/idTxtDevFullReturnsIndicadoresAverageSales"
                    android:layout_weight="0"
                    android:layout_gravity="center"
                    android:shadowColor="#000000"
                    android:textColor="#FFFFFF"
                    android:textStyle="bold" />
            </LinearLayout>

            <GridLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:columnCount="3"
                android:rowCount="5"
                android:id="@+id/gridLayout10"
                android:layout_below="@+id/idAmountProductReturn"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:rowOrderPreserved="true"
                android:columnOrderPreserved="true"
                android:useDefaultMargins="false"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="5dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="@string/txtDevFullDataSales"
                    android:id="@+id/textView83"
                    android:textStyle="normal"
                    android:textColor="#FFFFFF"
                    android:layout_weight="1"
                    android:layout_gravity="left|center_horizontal|right"
                    android:layout_row="0"
                    android:layout_column="0"
                    android:layout_marginBottom="7dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="@string/txtDevFullAmountSales"
                    android:id="@+id/textView84"
                    android:textStyle="normal"
                    android:textColor="#FFFFFF"
                    android:layout_weight="1"
                    android:layout_gravity="center_horizontal|right"
                    android:layout_column="1"
                    android:layout_row="0"
                    android:layout_marginLeft="10dp"
                    android:layout_marginBottom="7dp"
                    android:layout_columnSpan="2" />

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_column="0"
                    android:layout_row="1">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="X"
                        android:id="@+id/idTxtDevFullReturnsIndicadoresPercentageAllocated"
                        android:layout_gravity="left|center_vertical"
                        android:textColor="#FFFFFF" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="@string/txtDevFullBudgetSales"
                        android:id="@+id/textView85"
                        android:textStyle="normal"
                        android:textColor="#FFFFFF"
                        android:layout_gravity="left|center_vertical"
                        android:layout_row="1"
                        android:layout_column="0" />

                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="$"
                    android:id="@+id/textView88"
                    android:textColor="#FFFFFF"
                    android:layout_marginRight="5dp"
                    android:layout_column="1"
                    android:layout_row="1"
                    android:layout_marginLeft="10dp"
                    android:layout_gravity="left" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="36,00.00"
                    android:id="@+id/idTxtDevFullReturnsIndicadoresEstimateReturns"
                    android:textStyle="normal"
                    android:textColor="#FFFFFF"
                    android:layout_weight="1"
                    android:layout_column="2"
                    android:layout_row="1"
                    android:layout_gravity="right" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="@string/txtDevFullFoliosBudgetSales"
                    android:id="@+id/textView87"
                    android:textStyle="normal"
                    android:textColor="#FFFFFF"
                    android:layout_weight="1"
                    android:layout_gravity="left|center_vertical"
                    android:layout_row="2"
                    android:layout_column="0" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="$"
                    android:id="@+id/textView126"
                    android:textColor="#FFFFFF"
                    android:layout_marginRight="5dp"
                    android:layout_column="1"
                    android:layout_row="2"
                    android:layout_gravity="right"
                    android:layout_marginLeft="10dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="20,00.00"
                    android:id="@+id/idTxtDevFullReturnsIndicadoresFoliosInEstimate"
                    android:textStyle="normal"
                    android:textColor="#FFFFFF"
                    android:layout_weight="1"
                    android:layout_column="2"
                    android:layout_row="2"
                    android:layout_gravity="right" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="@string/txtDevFullFolioNoAceptSales"
                    android:id="@+id/textView91"
                    android:textStyle="normal"
                    android:textColor="#FFFFFF"
                    android:layout_weight="1"
                    android:layout_gravity="left|center_vertical"
                    android:layout_row="4"
                    android:layout_column="0"
                    android:visibility="invisible"
                    android:textSize="1sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="$"
                    android:id="@+id/textView127"
                    android:textColor="#FFFFFF"
                    android:layout_marginRight="5dp"
                    android:layout_column="1"
                    android:layout_row="4"
                    android:layout_gravity="right"
                    android:layout_marginLeft="10dp"
                    android:visibility="invisible"
                    android:textSize="1sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="4,00.00"
                    android:id="@+id/idTxtDevFullReturnsIndicadoresFoliosNoAccepted"
                    android:textStyle="normal"
                    android:textColor="#FFFFFF"
                    android:layout_weight="1"
                    android:layout_column="2"
                    android:layout_row="4"
                    android:layout_gravity="right"
                    android:visibility="invisible"
                    android:textSize="1sp" />
            </GridLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:layout_gravity="center_vertical"
                android:background="#7D7E7d"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:layout_marginBottom="5dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:layout_marginTop="5dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="@string/txtDevFullReturningAvailableSales"
                    android:id="@+id/textView94"
                    android:layout_weight="1"
                    android:layout_marginRight="5dp"
                    android:layout_gravity="center"
                    android:shadowColor="#000000"
                    android:textColor="#FFFFFF"
                    android:textStyle="bold" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="$"
                    android:id="@+id/textView93"
                    android:layout_weight="1"
                    android:shadowColor="#000000"
                    android:textColor="#FFFFFF"
                    android:textStyle="bold"
                    android:layout_gravity="center_vertical"
                    android:gravity="right"
                    android:layout_marginRight="10dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="10,000.00"
                    android:id="@+id/idTxtDevFullReturnsIndicadoresAvailableReturns"
                    android:layout_weight="0"
                    android:layout_marginRight="5dp"
                    android:layout_gravity="center_vertical"
                    android:shadowColor="#000000"
                    android:textColor="#FFFFFF"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:background="#7D7E7d"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:layout_marginTop="5dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:layout_marginBottom="5dp"
                android:layout_weight="1">

                <TextView
                    android:layout_width="222dp"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="@string/txtDevFullAuthorizedReturnSales"
                    android:id="@+id/textView95"
                    android:layout_gravity="center"
                    android:shadowColor="#000000"
                    android:textColor="#FFFFFF"
                    android:textStyle="bold"
                    android:singleLine="false" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="$"
                    android:id="@+id/textView90"
                    android:layout_weight="1"
                    android:shadowColor="#000000"
                    android:textColor="#FFFFFF"
                    android:textStyle="bold"
                    android:layout_gravity="center_vertical"
                    android:gravity="right"
                    android:layout_marginRight="10dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="10,000.00"
                    android:id="@+id/idTxtDevFullReturnsIndicadoresFoliosOutEstimation"
                    android:layout_marginRight="5dp"
                    android:layout_gravity="center_vertical"
                    android:shadowColor="#000000"
                    android:textColor="#FFFFFF"
                    android:textStyle="bold"
                    android:gravity="right" />
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:background="#7D7E7d"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:layout_marginTop="5dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:layout_marginBottom="5dp"
                android:layout_weight="1">

                <TextView
                    android:layout_width="222dp"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="@string/txtDevFullAuthorizedReturnDeclineSales"
                    android:id="@+id/textView79"
                    android:layout_gravity="center"
                    android:shadowColor="#000000"
                    android:textColor="#FFFFFF"
                    android:textStyle="bold"
                    android:singleLine="false" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="$"
                    android:id="@+id/textView86"
                    android:layout_weight="1"
                    android:shadowColor="#000000"
                    android:textColor="#FFFFFF"
                    android:textStyle="bold"
                    android:layout_gravity="center_vertical"
                    android:gravity="right"
                    android:layout_marginRight="10dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="4,00.00"
                    android:id="@+id/idTxtDevFullReturnsIndicadoresFoliosInMerma"
                    android:textStyle="bold"
                    android:textColor="#FFFFFF"
                    android:layout_column="2"
                    android:layout_row="3"
                    android:layout_gravity="center_vertical"
                    android:layout_marginRight="5dp" />

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:id="@+id/idButtonIndicadores"
            android:layout_marginBottom="10dp"
            android:layout_marginTop="5dp">

            <Button
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:text="@string/txtDevFullReturnsConsult"
                android:id="@+id/idBtnDevFullReturnsConsult"
                android:layout_weight="1"
                android:layout_gravity="center_horizontal"
                android:layout_alignParentBottom="true"
                android:textColor="#FFFFFF"
                android:background="@drawable/bluebutton"
                android:layout_alignParentLeft="true"
                android:layout_marginRight="5dp"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:paddingRight="5dp"
                android:paddingLeft="5dp"
                android:paddingBottom="3dp" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:text="@string/txtDevFullReturnsNew"
                android:id="@+id/idBtnDevFullNewReturn"
                android:layout_above="@+id/idBtnDevFullReturnsConsult"
                android:background="@drawable/bluebutton"
                android:textColor="#FFFFFF"
                android:layout_weight="1"
                android:layout_marginLeft="5dp"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:paddingLeft="2dp"
                android:paddingRight="2dp" />

        </LinearLayout>

    </RelativeLayout>

</LinearLayout>